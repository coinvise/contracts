{
  "name": "@coinvise/memberships",
  "description": "Coinvise contracts for Memberships",
  "version": "1.0.0",
  "private": true,
  "author": {
    "name": "Tokenfi Inc.",
    "email": "info@coinvise.co",
    "url": "https://github.com/coinvise"
  },
  "license": "UNLICENSED",
  "devDependencies": {
    "@codechecks/client": "^0.1.12",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@ethersproject/abi": "^5.5.0",
    "@ethersproject/abstract-signer": "^5.5.0",
    "@ethersproject/bignumber": "^5.5.0",
    "@ethersproject/bytes": "^5.5.0",
    "@ethersproject/constants": "^5.5.0",
    "@ethersproject/units": "^5.5.0",
    "@ethersproject/providers": "^5.5.3",
    "@ethersproject/wallet": "^5.5.0",
    "@nomiclabs/hardhat-ethers": "^2.0.4",
    "@nomiclabs/hardhat-etherscan": "^3.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.2",
    "@openzeppelin/contracts": "^4.6.0",
    "@openzeppelin/contracts-upgradeable": "^4.6.0",
    "@primitivefi/hardhat-dodoc": "^0.2.1",
    "@typechain/ethers-v5": "^9.0.0",
    "@typechain/hardhat": "^4.0.0",
    "@types/chai": "^4.3.0",
    "@types/fs-extra": "^9.0.13",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.18",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "chai": "^4.3.6",
    "commitizen": "^4.2.4",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "ethereum-waffle": "^3.4.0",
    "ethereumjs-util": "^7.1.4",
    "ethers": "^5.5.4",
    "fs-extra": "^10.0.0",
    "hardhat": "^2.8.4",
    "hardhat-gas-reporter": "^1.0.8",
    "hardhat-packager": "^1.3.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "lodash": "^4.17.21",
    "mocha": "^9.2.0",
    "nft.storage": "^6.3.0",
    "pinst": "^2.1.6",
    "prettier": "^2.8.4",
    "prettier-plugin-solidity": "^1.0.0-dev.21",
    "shelljs": "^0.8.5",
    "shx": "^0.3.4",
    "solc": "^0.8.12",
    "solhint": "^3.3.7",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.7.20",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.5.0",
    "typechain": "^7.0.0",
    "typescript": "^4.5.5"
  },
  "files": [
    "/artifacts",
    "/contracts",
    "/dist"
  ],
  "keywords": [
    "blockchain",
    "ethereum",
    "hardhat",
    "smart-contracts",
    "solidity"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "build:types": "pnpm prepare:types && tsc --project ./tsconfig.prod.json && copyfiles --up 1 \"./src/**/*.d.ts\" ./dist",
    "clean": "shx rm -rf ./artifacts ./cache ./coverage ./src/types ./dist ./coverage.json",
    "compile": "cross-env TS_NODE_TRANSPILE_ONLY=true hardhat compile",
    "coverage": "pnpm typechain && hardhat coverage --solcoverjs ./.solcover.js --temp artifacts --testfiles \"test/**/*.ts\" && pnpm typechain",
    "deploy:Memberships": "hardhat deploy:Memberships",
    "deploy:MembershipsFactory": "hardhat deploy:MembershipsFactory",
    "run:deployMemberships": "hardhat run:deployMemberships",
    "run:purchaseMembership": "hardhat run:purchaseMembership",
    "run:mintMembership": "hardhat run:mintMembership",
    "run:uploadMetadata:contract:nftStorage": "hardhat run:uploadMetadata:contract:nftStorage",
    "run:uploadMetadata:token:nftStorage": "hardhat run:uploadMetadata:token:nftStorage",
    "run:verifyMembershipsProxy": "hardhat run:verifyMembershipsProxy",
    "docs": "hardhat dodoc && prettier --config ./.prettierrc.yaml --write \"docs/**\" --loglevel silent",
    "lint": "pnpm lint:sol && pnpm lint:ts && pnpm prettier:check",
    "lint:sol": "solhint --config ./.solhint.json --max-warnings 0 \"contracts/**/*.sol\"",
    "lint:ts": "eslint --config ./.eslintrc.yaml --ignore-path ./.eslintignore --ext .js,.ts .",
    "postpack": "pinst --enable",
    "prepack": "pinst --disable && pnpm build:types",
    "prepare:types": "pnpm hardhat prepare-package",
    "prettier": "prettier --config ./.prettierrc.yaml --write \"**/*.{js,json,md,sol,ts}\"",
    "prettier:check": "prettier --check --config ./.prettierrc.yaml \"**/*.{js,json,md,sol,ts}\"",
    "prettier:silent": "pnpm prettier --loglevel silent",
    "test": "cross-env REPORT_GAS=true hardhat test",
    "typechain": "cross-env TS_NODE_TRANSPILE_ONLY=true hardhat typechain"
  }
}
